<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:Toggle label="Help" name="HelpToggle" style="justify-content: flex-end; flex-direction: row; align-items: stretch; align-self: flex-end; -unity-font-style: bold; -unity-text-align: upper-right;" />
    <ui:VisualElement name="HelpBox" style="flex-grow: 1; background-color: rgba(96, 96, 96, 0.72); margin-left: 30px; margin-right: 30px; margin-bottom: 30px; margin-top: 30px; visibility: hidden;">
        <ui:Label text="HelpBox Title" display-tooltip-when-elided="true" name="HelpTitle" style="-unity-font-style: bold; -unity-text-align: middle-center; color: rgb(188, 188, 188);" />
        <ui:TextField focusable="false" vertical-scroller-visibility="Auto" value="Help message" multiline="true" name="HelpTextField" max-length="-1" style="white-space: normal; color: rgb(0, 0, 0); -unity-background-image-tint-color: rgba(255, 255, 255, 0); flex-wrap: wrap; max-width: none; max-height: 80%;" />
        <ui:Button text="Close" display-tooltip-when-elided="false" name="CloseButton" enable-rich-text="false" />
    </ui:VisualElement>
</ui:UXML>
